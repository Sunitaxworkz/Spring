	</bean>


	<bean class="java.lang.Integer" id="i1">
		<constructor-arg value="500"></constructor-arg>
	</bean>
	<bean class="java.lang.Double" id="i2">
		<constructor-arg value="105.5"></constructor-arg>
	</bean>
	<bean class="java.lang.Float" id="i3">
		<constructor-arg value="4.09"></constructor-arg>
	</bean>
	<bean class="java.lang.Long" id="i4">
		<constructor-arg value="8050811032"></constructor-arg>
	</bean>
	<bean class="java.lang.Boolean" id="i5">
		<constructor-arg value="false"></constructor-arg>
	</bean>
	<bean class="java.lang.Short" id="i6">
		<constructor-arg value="10"></constructor-arg>
	</bean>



</beans>